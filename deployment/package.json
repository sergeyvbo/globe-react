{
  "name": "geoquiz-deployment",
  "version": "1.0.0",
  "description": "TypeScript deployment automation for GeoQuiz application",
  "type": "module",
  "scripts": {
    "deploy": "tsx scripts/deploy.ts",
    "build": "tsx scripts/build.ts",
    "config": "tsx scripts/config.ts",
    "secrets": "tsx scripts/utils/secrets.ts",
    "db:migrate": "tsx scripts/database.ts",
    "health-check": "tsx scripts/health-check.ts",
    "dev": "tsx scripts/deploy.ts --env development",
    "staging": "tsx scripts/deploy.ts --env staging",
    "production": "tsx scripts/deploy.ts --env production"
  },
  "dependencies": {
    "dockerode": "^4.0.2",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/dockerode": "^3.3.23",
    "tsx": "^4.6.2",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=20.6.0"
  }
}