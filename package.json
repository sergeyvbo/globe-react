{
  "name": "globe-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.13.5",
    "@emotion/styled": "^11.13.5",
    "@mui/icons-material": "^6.3.0",
    "@mui/material": "^6.3.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "d3": "^7.9.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0",
    "typescript": "^5.8.3",
    "web-vitals": "^4.2.4"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "download-flags": "node scripts/download-flags.js",
    "test-flags": "node scripts/test-flags.js",
    "flags-info": "node scripts/flags-info.js",
    "sonar:start": "docker-compose up -d sonarqube sonarqube-db",
    "sonar:stop": "docker-compose down sonarqube sonarqube-db",
    "analyze-frontend": "docker run --rm -v .:/usr/src -w /usr/src --network geoquiz-network -e SONAR_TOKEN sonarsource/sonar-scanner-cli -Dproject.settings=sonar-project-frontend.properties -Dsonar.host.url=http://sonarqube:9000",
    "analyze-backend": "docker run --rm -v .:/usr/src -w /usr/src/backend/GeoQuizApi --network geoquiz-network -e SONAR_TOKEN -e SONAR_HOST_URL=http://sonarqube:9000 mcr.microsoft.com/dotnet/sdk:10.0-preview sh -c \"dotnet tool install --global dotnet-sonarscanner && export PATH=\\\"$PATH:/root/.dotnet/tools\\\" && dotnet sonarscanner begin /k:geoquiz-backend /n:'GeoQuiz Backend' /v:1.0.0 /d:sonar.host.url=http://sonarqube:9000 /d:sonar.token=$SONAR_TOKEN && dotnet build && dotnet sonarscanner end /d:sonar.token=$SONAR_TOKEN\"",
    "analyze-all": "npm run analyze-frontend && npm run analyze-backend"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/d3": "^7.4.3",
    "@types/node": "^24.0.14",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/ui": "^3.2.4",
    "jsdom": "^26.1.0",
    "vite": "^7.0.4",
    "vitest": "^3.2.4"
  }
}